/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "compileOnSave": true,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "module": "es2020",
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@appServices/*": ["src/app/services/*"]
      ,"@appComponents/*": ["src/app/components/*"]
      ,"@appEnv/*": ["src/environments/*"]
      ,"@appResources/*": ["src/assets/*"]
    }
  }
}
